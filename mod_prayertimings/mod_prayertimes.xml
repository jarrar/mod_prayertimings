    </description>
    <files>
        <filename>mod_prayertimes.xml</filename>
        <filename module="mod_prayertimes">mod_prayertimes.php</filename>
        <filename>index.html</filename>
        <filename>helper.php</filename>
        <filename>tmpl/default.php</filename>
        <filename>tmpl/index.html</filename>
        <!--filename>conf/Settings.php</filename-->
        
    </files>
    <config>
    </config>
</extension>
