<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.1.0" client="site" method="upgrade">
    <name>Namaz Times</name>
    <author>Jarrar Jaffari (IABAT)</author>
    <version>0.0.1</version>
    <description>A simple Shia Mulsim prayer timing module for Joomla.
